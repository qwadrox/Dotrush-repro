{
    "name": "openapi-generator",
    "version": "1.0.0",
    "description": "OpenAPI Generator for Product Inventory API",
    "main": "index.js",
    "scripts": {
      "generate": "node generate.js",
      "generate:aspnetcore": "openapi-generator-cli generate -i ../openapi.yaml -g aspnetcore -o ./Generated --additional-properties=aspnetCoreVersion=8.0,buildTarget=library,classModifier=abstract,operationModifier=virtual,packageName=ProductInventoryApi,apiName=ProductInventory,returnICollection=true,useDefaultRouting=true,useSwashbuckle=true,nullableReferenceTypes=true,useDateTimeOffset=true,useNewtonsoft=true"
    },
    "keywords": [
      "openapi",
      "api",
      "generator"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
      "@openapitools/openapi-generator-cli": "^2.7.0"
    }
  }